{
  "tests": [
    {
      "name": "Handle OPTIONS request",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200,
      "bodyMatcher": "exact"
    },
    {
      "name": "Get bots for user",
      "method": "GET",
      "path": "/?user_id=999999",
      "expectedStatus": 200,
      "expectedBody": {
        "bots": []
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Create new bot should fail for duplicate unique_number",
      "method": "POST",
      "path": "/",
      "body": {
        "user_id": 1,
        "name": "Test Bot Duplicate",
        "telegram_token": "456:DEF",
        "template": "keys",
        "description": "Test bot for testing unique constraint",
        "logic": "Bot provides free and VIP keys to users",
        "unique_number": "123456"
      },
      "expectedStatus": 502,
      "bodyMatcher": "partial"
    }
  ]
}